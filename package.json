{
    "name": "netv",
    "version": "1.0.9",
    "description": "A network visualization library based on WebGL",
    "main": "build/NetV.js",
    "files": [
        "src",
        "build/*.js"
    ],
    "scripts": {
        "build": "webpack --env.watch=false",
        "build-production": "webpack --env.watch=false --env.mode=production",
        "watch": "webpack --env.watch=true",
        "eslint": "eslint src/index.ts",
        "build:benchmarks": "webpack --env.watch=false --config benchmarks/webpack.config.js",
        "watch:benchmarks": "webpack --env.watch=true --config benchmarks/webpack.config.js",
        "build:test": "webpack --env.watch=false --config test/webpack.config.js",
        "watch:test": "webpack --env.watch=true --config test/webpack.config.js"
    },
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "@types/jest": "^25.2.3",
        "@types/jsdom": "^16.2.3",
        "@typescript-eslint/eslint-plugin": "^3.0.0",
        "@typescript-eslint/parser": "^3.0.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.0.1",
        "eslint": "^7.1.0",
        "eslint-config-alloy": "^3.7.2",
        "eslint-plugin-jest": "^23.13.1",
        "jest": "^26.0.1",
        "jsdom": "^16.2.2",
        "raw-loader": "^4.0.1",
        "ts-jest": "^26.0.0",
        "ts-loader": "^7.0.4",
        "typescript": "^3.9.3",
        "webpack": "^4.43.0",
        "webpack-bundle-analyzer": "^3.8.0",
        "webpack-cli": "^3.3.12"
    },
    "dependencies": {
        "d3": "^6.2.0"
    }
}
